# Режим работы с моками (без обращения к бекенду)
USE_MOCKS=true

# Версия API (должна совпадать с версией, использованной при сборе моков)
VERSION=v1

# Серверные переменные - не нужны при работе с моками
# HOST можно оставить пустым или не указывать
HOST=
# HOSTNAME и PORT также не нужны
# HOSTNAME=
# PORT=

# Клиентские переменные - не нужны при работе с моками
# NEXT_PUBLIC_HOST2 можно оставить пустым или не указывать
NEXT_PUBLIC_HOST2=
# NEXT_PUBLIC_HOSTNAME и NEXT_PUBLIC_PORT также не нужны
# NEXT_PUBLIC_HOSTNAME=
# NEXT_PUBLIC_PORT=

# Настройки cookies (могут быть нужны для работы приложения)
# Используйте localhost для локальной разработки
NEXT_PUBLIC_HOST=http://127.0.0.1
NEXT_PUBLIC_MAX_AGE=2678400
NEXT_PUBLIC_DOMAIN=localhost
NEXT_PUBLIC_SECURE=false

